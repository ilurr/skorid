<!DOCTYPE html>
<html lang="en">
	{{> head}}

<body>
	<!-- header -->
	{{> header section_name="Kompetisi"}}
	
	<!-- ads skin -->
	{{!> ads_skin}}
	
	<div class="row clearfix">
		<div class="container clearfix">
			<!-- ads top -->
			{{> ads_top}}
		</div>
	</div>

	<!-- title -->
	<div class="row clearfix">
		<div class="container clearfix">
			<div class="col-offset-fluid clearfix">
				<div class="col-bs10-10">
			
					<div class="title3">
						<span>MotoGP</span>
					</div>

				</div>
			</div>
		</div>
	</div>
	
	<!-- content -->
	<div class="row clearfix">
		<div class="container clearfix">
			<div class="col-offset-fluid clearfix">
				
				<div class="col-bs10-7">
				
					<section class="tab -page box clearfix">
						<div class="tab__menu clearfix">
							<div class="tab__item">
								<a href="kompetisi-detail-motogp.html" class="tab__link -active"><div class="title"><span>Balapan</span></div></a>
							</div>
							<div class="tab__item">
								<a href="kompetisi-klasemen-motogp-riders.html" class="tab__link"><div class="title"><span>Klasemen Pembalap</span></div></a>
							</div>
							<div class="tab__item">
								<a href="kompetisi-klasemen-motogp-team.html" class="tab__link"><div class="title"><span>Klasemen Tim</span></div></a>
							</div>
						</div>
						<div class="tab__content">
							<div class="tab__content__item">

								<!-- schedule -->
								<div class="schedule__gameweek">
									{{> widget_table_motogp channel=channel.balap.motogp index_start=0 index_end=10 detail=true}}
								</div>

							</div>
						</div>
					</section>

				</div>
				
				<div class="col-bs10-3">
				
					<!-- ads giant -->
					{{> ads_giant}}
					
				</div>
			</div>
		</div>
	</div>

	<!-- footer -->
	{{> footer}}
	
	<!-- ads horizontal -->
	{{> ads_horizontal}}

	{{> js}}
	
</body>
</html>
