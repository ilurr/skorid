<!DOCTYPE html>
<html lang="en">
	{{> head}}

<body>
	<!-- header -->
	{{> header section_name="Klasemen"}}
	
	<!-- ads skin -->
	{{!> ads_skin}}
	
	<div class="row clearfix">
		<div class="container clearfix">
			<!-- ads top -->
			{{> ads_top}}
		</div>
	</div>

	<!-- belt schedule -->
	<div class="row belt clearfix">
		<div class="container clearfix">
			<div class="col-offset-fluid clearfix">
				<div class="col-bs10-10">
				
					<!-- schedule -->
					<section class="schedule box clearfix">
						<div class="schedule__container">
							<div class="schedule__wrap js--schedule">
								<div class="schedule__item">
									{{> widget_salat channel=channel.bola.fixtures.inggris.rounds.[0] index_start=0 index_end=4}}
								</div>
								<div class="schedule__item">
									{{> widget_salat channel=channel.bola.fixtures.inggris.rounds.[1] index_start=0 index_end=4}}
								</div>
							</div>
						</div>
					</section>
					
				</div>
			</div>
		</div>
	</div>
	
	<!-- title -->
	<div class="row clearfix">
		<div class="container clearfix">
			<div class="col-offset-fluid clearfix">
				<div class="col-bs10-10">
			
					<div class="title3">
						<span>Klasemen</span>
					</div>

				</div>
			</div>
		</div>
	</div>
	
	<!-- content -->
	<div class="row clearfix">
		<div class="container clearfix">
			<div class="col-offset-fluid clearfix">
				
				<div class="col-bs10-7">
				
					<!-- standing -->
					<section class="standing box clearfix">
						{{> widget_valas channel=channel.bola.team.inggris index_start=0 index_end=20 league="Inggris" detail=true}}
					</section>
					
					<section class="standing box clearfix">
						<div class="standing__info">
							<div class="k-col">
								<h4>Keterangan</h4>
								<ul>
									<li><strong>M</strong> : Main</li>
									<li><strong>M</strong> : Menang</li>
									<li><strong>S</strong> : Seri</li>
									<li><strong>K</strong> : Kalah</li>
									<li><strong>GM</strong> : Gol Memasukkan</li>
									<li><strong>GK</strong> : Gol Kemasukan</li>
									<li><strong>SG</strong> : Selisih Gol</li>
									<li><strong>P</strong> : Poin</li>
								</ul>
							</div>
							<div class="k-col">
								<h4>Keterangan</h4>
								<ul>
									<li><span class="badge ucl"></span> Babak Grup Liga Champions</li>
									<li><span class="badge uel"></span> Babak Grup Liga Eropa</li>
									<li><span class="badge uel2"></span> Kualifikasi Liga Eropa</li>
									<li><span class="badge deg"></span> Degradasi</li>
								</ul>
							</div>
						</div>
					</section>
					
				</div>
				
				<div class="col-bs10-3">
				
					<!-- ads giant -->
					{{> ads_giant}}
					
				</div>
			</div>
		</div>
	</div>

	<!-- footer -->
	{{> footer}}
	
	<!-- ads horizontal -->
	{{> ads_horizontal}}

	{{> js}}
	
</body>
</html>
